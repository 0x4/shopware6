{
  "name": "shopware/platform",
  "type": "library",
  "description": "The Shopware e-commerce core",
  "keywords": ["framework", "shopware"],
  "homepage": "https://www.shopware.com",
  "license": [
    "AGPL-3.0",
    "proprietary"
  ],
  "replace": {
    "shopware/core": "self.version",
    "shopware/storefront": "self.version",
    "shopware/administration": "self.version"
  },
  "require": {
    "php": ">=7.1.3",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "ext-zip": "*",
    "cocur/slugify": "3.1",
    "doctrine/dbal": "2.9.2",
    "league/flysystem": "1.0.49",
    "league/flysystem-aws-s3-v3": "1.0.21",
    "ramsey/uuid": "3.8.0",
    "sensio/framework-extra-bundle": "5.2.4",
    "superbalist/flysystem-google-storage": "7.1.0",
    "symfony/asset": "4.2.2",
    "symfony/config": "4.2.2",
    "symfony/console": "4.2.2",
    "symfony/debug-bundle": "4.2.2",
    "symfony/dependency-injection": "4.2.2",
    "symfony/dotenv": "4.2.2",
    "symfony/framework-bundle": "4.2.2",
    "symfony/http-foundation": "4.2.2",
    "symfony/http-kernel": "4.2.2",
    "symfony/monolog-bundle": "3.3.1",
    "symfony/options-resolver": "4.2.2",
    "symfony/process": "4.2.2",
    "symfony/routing": "4.2.2",
    "symfony/serializer-pack": "1.0.2",
    "symfony/serializer": "4.2.2",
    "symfony/property-access": "4.2.2",
    "symfony/property-info": "4.2.2",
    "symfony/swiftmailer-bundle": "3.2.5",
    "symfony/templating": "4.2.2",
    "symfony/translation": "4.2.2",
    "symfony/twig-bundle": "4.2.2",
    "symfony/validator": "4.2.2",
    "symfony/yaml": "4.2.2",
    "symfony/messenger": "4.2.2",
    "twig/extensions": "1.5.4",
    "twig/twig": "2.6.2",
    "voku/stop-words": "2.0.0",
    "league/oauth2-server": "7.3.2",
    "symfony/psr-http-message-bridge": "1.1.0",
    "zendframework/zend-diactoros": "1.0.2",
    "doctrine/inflector": "1.3.0",
    "phpoffice/phpword": "0.15.0",
    "smalot/pdfparser": "0.13.2",
    "james-heinrich/getid3": "1.9.16",
    "enqueue/messenger-adapter": "0.1.2"
  },
  "require-dev": {
    "ext-tokenizer": "*",
    "ext-xmlwriter": "*",
    "bheller/images-generator": "1.0.1",
    "fzaninotto/faker": "1.8.0",
    "mbezhanov/faker-provider-collection": "1.1.1",
    "phpunit/phpunit": "7.5.2",
    "symfony/browser-kit": "4.2.2",
    "symfony/phpunit-bridge": "4.2.2",
    "symfony/var-dumper": "4.2.2",
    "symfony/stopwatch": "4.2.2",
    "symfony/web-profiler-bundle": "4.2.2",
    "johnkary/phpunit-speedtrap": "3.0.0",
    "league/flysystem-memory": "1.0.1"
  },
  "conflicts": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "Shopware\\Administration\\": "src/Administration/",
      "Shopware\\Storefront\\": "src/Storefront/",
      "Shopware\\Core\\": "src/Core/"
    },
    "exclude-from-classmap": [
      "src/**/Test/"
    ]
  }
}
