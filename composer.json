    "sensio/framework-extra-bundle": "^5.1",
    "superbalist/flysystem-google-storage": "^5.0",
    "symfony/asset": "^4.0",
    "symfony/config": "^4.0",
    "symfony/console": "^4.0",
    "symfony/debug-pack": "^1.0",
